{
  "name": "mini-tube",
  "version": "1.0.1",
  "bin": {
    "mini-tube": "bin/main.js"
  },
  "dependencies": {
    "electron": "^3.0.5",
    "keybinds": "^1.1.1",
    "mouse-activity": "^1.0.2",
    "reset-css": "^4.0.1",
    "wigly": "^0.1.21",
